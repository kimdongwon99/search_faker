<div class="container">
<h1>결과창</h1>
<table>
    <thead>
        <tr>
            <th>글 번호</th>
            <th>제목</th>
            <th>작성일자</th>
            

        </tr>
    </thead>
    <tbody>
        <% @posts.each do |p| %>
        <tr>
            <td><%= p.id%></td>
            <td><%= p.title%></td>
            <td><%=p.created_at.strftime("%y=%m-%d") %></td>
            <% end%>
        </tr>
    </tbody>
</table>

</div>